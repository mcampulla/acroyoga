@{
    ViewBag.Title = "Acroyoga Padova";
    Layout = "~/Views/Shared/_LayoutTemp.cshtml";
}
<header id="myCarousel" class="carousel slide">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <!-- Wrapper for Slides -->
    <div class="carousel-inner">
        <div class="item active">
            <!-- Set the first background image using inline CSS below. -->
            <div class="fill" style="background-image:url('/Content/images/main5.jpg');"></div>

            <div class="out">
                <img src="~/Content/images/logo-home-black1.png" class="img-responsive">
            </div>
        </div>
        <div class="item">
            <div class="fill" style="background-image:url('/Content/images/main11.jpg');"></div>
            <div class="out">
                <img src="~/Content/images/logo-home-black1.png" class="img-responsive">
            </div>
        </div>
        <div class="item">
            <div class="fill" style="background-image:url('/Content/images/main7.jpg');"></div>
            <div class="out">
                <img src="~/Content/images/logo-home-black1.png" class="img-responsive">
            </div>
        </div>
    </div>
    <!-- Controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="icon-prev"></span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="icon-next"></span>
    </a>
</header>
